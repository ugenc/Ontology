        <rdfs:domain rdf:resource="https://ci.mines-stetienne.fr/seas/ElectricPowerSystem"/>
        <rdfs:range rdf:resource="https://ci.mines-stetienne.fr/seas/ElectricPowerProperty"/>
        <rdfs:comment xml:lang="en">Links an electric power system to the consumed electric power.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ci.mines-stetienne.fr/seas/ElectricPowerSystemOntology"/>
        <rdfs:label>consumed electric power</rdfs:label>
        <ns:term_status>testing</ns:term_status>
    </owl:ObjectProperty>
    


    <!-- https://ci.mines-stetienne.fr/seas/derivesFrom -->

    <owl:ObjectProperty rdf:about="https://ci.mines-stetienne.fr/seas/derivesFrom">
        <owl:inverseOf rdf:resource="https://ci.mines-stetienne.fr/seas/derivesFrom"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
        <rdfs:domain rdf:resource="https://ci.mines-stetienne.fr/seas/Property"/>
        <rdfs:range rdf:resource="https://ci.mines-stetienne.fr/seas/Property"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://ci.mines-stetienne.fr/seas/summedProperty"/>
            </rdf:Description>
            <rdf:Description rdf:about="https://ci.mines-stetienne.fr/seas/sumProperty"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">Links a seas:Property of another seas:Property it derives from.

For example,

```
&lt;fridge/1/consumptionPower&gt; seas:derivesFrom &lt;fridge/1/consumedEnergy&gt; .
```

This property is symmetric, and the derived property is also a property of the feature of interest:

```
seas:hasProperty &lt; seas:hasProperty o seas:derivesFrom
```

</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://ci.mines-stetienne.fr/seas/FeatureOfInterestOntology"/>
        <rdfs:label xml:lang="en">derives from</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


