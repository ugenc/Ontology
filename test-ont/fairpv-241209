    <!-- file:/Users/ugenc/Downloads/timeDerivative -->

    <owl:ObjectProperty rdf:about="file:/Users/ugenc/Downloads/timeDerivative">
        <rdfs:subPropertyOf rdf:resource="file:/Users/ugenc/Downloads/derivesFrom"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:comment xml:lang="en">Links a time relative property to its time derivative property. For example, 

```
&lt;car/1/speed&gt; seas:timeDerivative &lt;car/1/acceleration&gt; .
```

Means that `&lt;car/1/acceleration&gt;` is the time derivative of `&lt;car/1/speed&gt;`.
</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="file:/Users/ugenc/Downloads/TimeOntology"/>
        <rdfs:label xml:lang="en">time derivative</rdfs:label>
        <ns:term_status>stable</ns:term_status>
    </owl:ObjectProperty>
    


