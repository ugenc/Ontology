    <!-- http://www.w3.org/ns/sosa/Result -->

    <owl:Class rdf:about="http://www.w3.org/ns/sosa/Result">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/isResultOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/RePlanIT/Activity"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/sosa/isResultOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/ns/sosa/Observation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>The result of an activity, process or sensor observation.</dc:description>
        <rdfs:comment>The value 20 as the height of a certain tree together with the unit, e.g., Meter.</rdfs:comment>
        <rdfs:label>Result</rdfs:label>
    </owl:Class>
    


